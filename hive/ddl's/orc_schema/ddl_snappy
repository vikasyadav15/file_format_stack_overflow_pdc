  `lastaccessdate` string,
  `websiteurl` varchar(256),
  `location` varchar(256),
  `aboutme` string,
  `views` int,
  `upvotes` int,
  `downvotes` int,
  `profileimageurl` varchar(200),
  `age` int,
  `accountid` int)
STORED AS ORC
