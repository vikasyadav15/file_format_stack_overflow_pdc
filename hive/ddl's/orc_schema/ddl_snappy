CREATE TABLE `badges_orc_snappy`(
  `id` int,
  `userid` int,
  `name` varchar(50),
  `date` string,
  `class` int,
  `tagbased` boolean)
STORED AS ORC
LOCATION
  '/user/hive/warehouse/stackoverflow.db/badges_orc_snappy'
  TBLPROPERTIES (
    'orc.compress'='SNAPPY');


CREATE TABLE `comments_orc_snappy`(
  `id` int,
